{
  "name": "metamodels/attribute_combinedvalues",
  "description": "MetaModels attribute for combinedvalues",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "combinedvalues"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_combinedvalues/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_combinedvalues"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev",
    "symfony/dependency-injection": "^3.3 || ^4.0",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4.0"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.2",
    "contao/manager-bundle": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeCombinedValuesBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeCombinedValuesBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeCombinedValuesBundle\\ContaoManager\\Plugin",
    "contao": {
      "runonce": [
        "runonce/runonce.php"
      ],
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_combinedvalues-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-feature/contao4": "2.1.x-dev"
    }
  }
}
